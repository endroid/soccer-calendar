{
    "name": "endroid/soccer-calendar",
    "description": "Endroid Soccer Calendar",
    "keywords": ["endroid", "soccer", "calendar", "php"],
    "homepage": "https://github.com/endroid/soccer-calendar",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl"
        }
    ],
    "require": {
        "php": "^8.1",
        "endroid/calendar": "^3.2",
        "endroid/soccer-data": "^1.3.2",
        "symfony/uid": "^5.4||^6.4||^7.0"
    },
    "require-dev": {
        "endroid/quality": "dev-master"
    },
    "suggest": {
        "roave/security-advisories": "Avoids installation of package versions with vulnerabilities"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\SoccerCalendar\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\SoccerCalendar\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "endroid/installer": true
        }
    }
}
